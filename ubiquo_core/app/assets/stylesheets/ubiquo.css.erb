/*
 *= require ubiquo/ubiquo
 *= require ubiquo/listings
 *= require ubiquo/lightwindow
 *= require ubiquo/ubiquo_lightwindow 
 */
 
 <% 
    env      = asset_environment rescue {}
    plugins  = []
    plugins += Ubiquo::Plugin.registered_plugins.reject { |a| a == :ubiquo }.select { |a| env[a.to_s] }
    
    plugins.each do |plugin|
      require_asset(plugin.to_s)
    end    
 %>
